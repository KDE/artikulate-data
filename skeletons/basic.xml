    <phrase>
     <id>{dd2695dc-7fb7-4a71-8bca-0c6a186f82ee}</id>
     <text>How far is the train station?</text>
     <type>sentence</type>
    </phrase>
    <phrase>
     <id>{eb292974-a18e-4e48-8237-bb480c7e5f0f}</id>
