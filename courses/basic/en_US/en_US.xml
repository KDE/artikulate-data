    <phrase>
     <id>{5e11f35b-c7b7-4ad4-a0f1-40564bf8ae48}</id>
     <foreignId>{a8efb03d-9e2e-4cbc-bdb1-ed7d54e8df5c}</foreignId>
     <text>Go over the bridge.</text>
